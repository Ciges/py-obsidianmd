{
    "tests": {
        "default_meta_type": ">>MetadataType.ALL",
        "tests-remove_duplicate_values": {
            "n4_1": {
                "description": "on all keys and all metadata types",
                "data": "n4",
                "inputs": {
                    "k": null,
                    "meta_type": null
                },
                "expected_output": {
                    "frontmatter": {
                        "tags": [
                            "t1",
                            "t2",
                            "t3"
                        ],
                        "fm": [
                            "hello, world, hello"
                        ]
                    },
                    "inline": {
                        "c1": [
                            "foo",
                            "foo bar"
                        ],
                        "c2": [
                            "foo",
                            "foo bar",
                            "biz",
                            "baz"
                        ],
                        "c3": [
                            "3",
                            "4",
                            "5"
                        ],
                        "tags": [
                            "t4",
                            "t5",
                            "t6"
                        ]
                    }
                }
            },
            "n4_2": {
                "description": "on 1 specific value of the frontmatter",
                "data": "n4",
                "inputs": {
                    "k": "tags",
                    "meta_type": ">>MetadataType.FRONTMATTER"
                },
                "expected_output": {
                    "frontmatter": {
                        "tags": [
                            "t1",
                            "t2",
                            "t3"
                        ],
                        "fm": [
                            "hello, world, hello"
                        ]
                    },
                    "inline": {
                        "c1": [
                            "foo",
                            "foo bar"
                        ],
                        "c2": [
                            "foo",
                            "foo bar",
                            "biz",
                            "baz"
                        ],
                        "c3": [
                            "3",
                            "4",
                            "5",
                            "5"
                        ],
                        "tags": [
                            "t4",
                            "t5",
                            "t6",
                            "t5"
                        ]
                    }
                }
            }
        },
