        "tests-order_keys": {
            "n7_1": {
                "description": "order only frontmatter",
                "data": "n7",
                "inputs": {
                    "how": ">>Order.ASC",
                    "meta_type": ">>MetadataType.FRONTMATTER"
                },
                "expected_output": {
                    "inline": [
                        "i2",
                        "i0",
                        "i1"
                    ],
                    "frontmatter": [
                        "f0",
                        "f1",
                        "f2"
                    ]
                }
            },
            "n7_2": {
                "description": "order only inline",
                "data": "n7",
                "inputs": {
                    "how": ">>Order.ASC",
                    "meta_type": ">>MetadataType.INLINE"
                },
                "expected_output": {
                    "inline": [
                        "i0",
